# function to create frames from a movie
function mkFrames(){ 	USAGE='ERROR:\tPlease supply:\n\t- the name of the file you would like to convert.  \n\t     EX:  "mkFrames foo.mp4"'; 	if [ -n "$1" ] ; then 		dest="`pwd`/$1_frames"; 		echo $dest; 		mkdir $dest; 		echo "Making frames from movie $1"; 		echo "Frames will be in $dest"; 		sleep 1; 		echo "Starting...."; 		sleep 1; 		cmd="ffmpeg -i $1 -r 24.0 $dest/$1.%4d.png"; 		echo $cmd; 		ffmpeg -i $1 -r 24.0 $dest/$1.%4d.png; 		 		echo ""; 		echo ""; 		echo "Made movie from $1"; 		echo "Frames are in $dest";        else 		echo -e $USAGE; 		OK=0; 	fi; }
# function to create a movie from frames
function mkMovie(){ 	USAGE='ERROR:\tPlease supply:\n\t- a path to the frames you would like to convert\n\t- start frame\n\n\tImages should be in format \"prefix.counter.extension\"\n\t\tEX: foo.1001.exr\n\nEX:\tAssuming your files are in directory /home/$USER/temp:\n\tmkMovie /home/$USER/temp 1001\n...or....\n\tmkMovie `pwd` 1001'; 	SF=1001; 	OK=1; 	echo ""; 	echo "============================================="; 	if [ -n "$1" ] ; then 		if [ -d $1 ]; then 			if [ -n "$2" ]; then 				if [ "$2" -eq "$2" ] 2>/dev/null; 				then 					SF="$2"; 					OK=1; 				else 					echo "ERROR:  second parameter must be an integer."; 					echo -e $USAGE; 					OK=0; 				fi; 			fi; 		else 			echo 'ERROR:  No such directory "$1"'; 			echo -e $USAGE; 			OK=0; 		fi; 	else 		echo -e $USAGE; 		OK=0; 	fi; 	if [ $OK -eq "1" ]; then 		cd $1 ; 		for x in `ls -1` ; do 			FFN=$(basename -- "$x"); 			SSUFFIX="${FFN##*.}"; 			if [ $SSUFFIX == "mp4" ]; then 				echo "NOTE:  skipping existing mp4 file: $x"; 			else 				lastfile="$x"; 			fi; 		done; 		FN=$(basename -- "$lastfile"); 		SUFFIX="${FN##*.}"; 		PREFIX="${FN%%.*}"; 		 		extra=""; 		if [ $SUFFIX == "exr" ]; then 			echo "NOTE:  EXR files detected, assuming linear, adding \"-gamma 2.2\""; 			extra="-gamma 2.2"; 		fi; 		 		CMD="ffmpeg -start_number $SF $extra -i $1/$PREFIX.%04d.$SUFFIX -vcodec png $PREFIX.mp4"; 		 		echo $CMD; 		echo "============================================="; 		echo ""; 		$CMD; 	fi; }
