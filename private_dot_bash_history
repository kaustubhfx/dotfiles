# jump to dailies dir
function day(){   if [ -d /${LB_ROOT}/FX/STUDENTS/$LBCLASS/DAILIES ]; then       cd  /${LB_ROOT}/FX/STUDENTS/$LBCLASS/DAILIES;       for curr in `ls -1` ; do       	fin="$curr";       done;                    if [ -d $fin ]; then       	 cd $fin;       fi;   fi; }
# jump to delivery dir
function del(){   if [ -d /${LB_ROOT}/FX/STUDENTS/$LBCLASS/DELIVERY ]; then       cd  /${LB_ROOT}/FX/STUDENTS/$LBCLASS/DELIVERY;   fi; }
# load last saved hip file
function hl(){   if [ -d $HIP ]; then       for curr in `ls -tr1 $HIP/*hip*` ; do       	if [ -f $curr ]; then       	    fin="$curr";       	fi;       done;              cd $HIP;       if [ -e $fin ]; then       	echo "Starting last saved file $fin";       	houdini -n $fin;       else       	echo "No files here, starting blank houdini";       	houdini;       fi;   fi; }
# load last saved nuke file
function nl(){   finalfile="";   if [ -d $NUKE ]; then       for curr in `ls -tr1 $NUKE/*nk*` ; do       	if [ -f $curr ]; then       	    finalfile="$curr";       	fi;       done;              cd $NUKE;              if [ "$finalfile" != "" ]; then       	echo "Starting last saved file: $finalfile";       	nk $finalfile &       else       	echo "No files here, starting blank nuke.";       	nk;       fi;   fi; }
function rhq(){   echo "-- Preparing to restart hqclient on $1";   if [ $1 == "all" ] ; then   	sudo -u hquser /${LB_ROOT}/LIBRARY/TECH_CONFIG/PIPELINE/hqueue/hqclient_ssh_rc.sh all restart;   else   	sudo -u hquser /${LB_ROOT}/LIBRARY/TECH_CONFIG/PIPELINE/hqueue/hqclient_ssh_rc.sh $1 restart;   fi;    }
