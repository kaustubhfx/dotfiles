def registerTypes(type_registry):
    # Check if the visualizer already exists before registering
    if not type_registry.hasVisualizer("genericvisualizer"):
        type_registry.registerVisualizer(GenericVisualizer)
python def registerTypes(type_registry):
    # Check if the visualizer already exists before registering
    if not type_registry.hasVisualizer("genericvisualizer"):
        type_registry.registerVisualizer(GenericVisualizer)
python3 def registerTypes(type_registry):
    # Check if the visualizer already exists before registering
    if not type_registry.hasVisualizer("genericvisualizer"):
        type_registry.registerVisualizer(GenericVisualizer)
