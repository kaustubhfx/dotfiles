# function to create archive of a folder
function packit(){ 	f="/tmp/$USER.`date +\%Y-\%m-\%d-\%T`.tar.gz"; 	src="/${LB_ROOT}/FX/STUDENTS/.$LBCLASS/$USER"; 	if [ -n "$1" ] ; then 		src="$1"; 	fi; 	 	if [ ! -d $src  ]; then 		echo "ERROR:  No such directory $1"; 		return 1; 	fi; 	 	echo ""; 	echo "INFO:        Preparing to save archive."; 	echo "Source:      $src"; 	echo "Destination: $f"; 	echo ""; 	sleep 1; 	read -p "OKAY to continue?  Press ENTER to continue, any other key to exit." ok; 	 	if [ ! -n "$ok" ]; then 			if [ -d $src  ]; then 				echo ""; 				tar -zcvf $f $src; 				echo ""; 				echo "Archive saved out to:  $f"; 			else 			  	echo "ERROR:  No such directory $1"; 			fi; 	fi; 	 }
function discusage(){
        export LB_LOSTBOYS_USAGE=`df 2> >(grep -v 'Permission denied')| grep $2 | tr -s ' ' | cut -d ' ' -f 5 | cut -d '%' -f 1 2>/dev/null`;  	OK=1
	[ -n "$LB_LOSTBOYS_USAGE" ] && [ "$LB_LOSTBOYS_USAGE" -eq "$LB_LOSTBOYS_USAGE" ] 2>/dev/null; 	if [ $? -ne 0 ]; then
	   OK=0; 	fi;  	if [ $OK -eq 1 ]; then 		if [ "$LB_LOSTBOYS_USAGE" -gt "$1" ]; then 			echo -e "--> \e[41;5m$2 : ${LB_LOSTBOYS_USAGE}%\e[0m"; 		else 			echo "--> $2 : ${LB_LOSTBOYS_USAGE}%"; 		fi;         fi; }
echo ""
